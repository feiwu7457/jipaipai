@import "../diy/diy.wxss";
.pagetop .slide-image {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}

.pagetop .swiper-box {
  height: 285rpx;
  padding: 95rpx 20rpx 0;
}

.pagetop .swiper-box swiper {
  height: 100%;
  border-radius: 20rpx;
}

.pagetop .swiper-box .wx-swiper-dot {
  width: 10rpx;
  display: inline-flex;
  height: 10rpx;
  justify-content: space-between;
  border-radius: 0;
  position: relative;
  top: -70rpx;
  z-index: 8;
}

.pagetop  .swiper-box .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(255, 255, 255, 0.5);
  border-radius: 0;
}

.pagetop .swiper-box .wx-swiper-dot-active {
  width: 28rpx;
}

.pagetop .swiper-box .wx-swiper-dot-active::before {
  background: rgba(255, 255, 255, 1);
  border-radius: 0;
}

.gosearch {
  position: absolute;
  z-index: 10;
  margin: 15rpx 30rpx;
  right: 0;
  left: 0;
  height: 60rpx;
  line-height: 60rpx;
  border: 1px solid #e3506d;
  /* background: rgba(0, 0, 0, 0.6); */
  color: #999;
  padding-left: 30rpx;
  border-radius:30rpx;
  background-position: 34rpx center;
  background-size: 40rpx 40rpx;
  background-repeat: no-repeat;
}

.gosearch image{
  width: 35rpx;
  height: 35rpx;
  margin-right: 20rpx;
}

.index_card_01 {
  position: relative;
  z-index: 9;
  margin: 10rpx 0 10rpx;
}

.index_card_01 .flex_item .icon {
  margin-bottom: 18rpx;
}

.pagetop {
  position: relative;
  padding-bottom: 10rpx;
}

.jrms_box {
  position: relative;
}

.jrms_box swiper {
  height: 340rpx;
}

.jrms_box .jrms_hd {
  display: flex;
  padding: 34rpx 30rpx;
  align-items: center;
}

.jrms_box .jrms_hd .left {
  display: flex;
  flex: 1;
  font-size: 36rpx;
  font-weight: 600;
  align-items: center;
}

.jrms_box .jrms_hd .more {
  font-size: 26rpx;
  color: #666;
  position: relative;
}

.jrms_box .jrms_hd .more::after {
  content: "";
  display: inline-block;
  position: relative;
  top: -3rpx;
  width: 10rpx;
  height: 10rpx;
  border-style: solid;
  border-width: 2rpx;
  border-color: #333 #333 transparent transparent;
  transform: rotate(45deg);
  margin-left: 10rpx;
}

.countdown {
  position: relative;
  margin-left: 10rpx;
  font-size: 24rpx;
  top: -3rpx;
}

.countdown text {
  display: inline-block;
  background-color: #333;
  height: 40rpx;
  line-height: 44rpx;
  color: #fff;
  font-size: 24rpx;
  padding: 0 8rpx;
  margin: 0 4rpx;
  vertical-align: middle;
  position: relative;
  top:3rpx;
  font-family: "num";
}

.slide2-image {
  width: 100%;
  height: 200rpx;
  border-radius: 15rpx 15rpx 0 0;
}

.jrms_box .jrms_bd {
  padding-left: 30rpx;
}

.mspor_item {
  margin-right: 10rpx;
}

.mspor_item .imgbox {
  width: 100%;
  height: 0;
  padding-bottom: 100%;
  overflow: hidden;
  position: relative;
  border-radius: 20rpx 20rpx 0 0;
}

.mspor_item .imgbox image {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.mspor_item .textbox {
  padding: 20rpx 10rpx;
}

.mspor_item .title {
  font-size: 28rpx;
  line-height: 1em;
}

.mspor_item .price {
  color: #f65236;
}
